<project name="Downloadjpg" default="filter">

<target name="clean">
<delete dir="jpgfiles"/>
</target>

<target name="init" depends="clean">
<mkdir dir="jpgfiles"/>
</target>

<target name="filter" depends="init">

<copydir src="DownloadFiles"
           dest="jpgfiles"
           includes="*.jpg"/>

</target>

</project>
